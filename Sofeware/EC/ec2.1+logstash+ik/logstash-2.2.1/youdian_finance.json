{
  "template": "youdian_life",
  "settings" : {
    "index.refresh_interval" : "1s"
  },
  "mappings": {
    "youdian_finance": {
      "_all": {
        "analyzer": "ik_max_word",
        "search_analyzer": "ik_max_word",
        "term_vector": "no",
        "store": "false"
      },
      "properties" : {
        "@version": { "type": "string", "index": "not_analyzed" },
        "username":{"type": "string", "store": "yes","term_vector": "with_positions_offsets", "analyzer": "ik_max_word","search_analyzer": "ik_max_word","include_in_all":"true","boost": 8},
        "agentname":{"type": "string", "store": "yes","term_vector": "with_positions_offsets", "analyzer": "ik_max_word","search_analyzer": "ik_max_word","include_in_all":"true","boost": 8}
       }
    }
  }
}
